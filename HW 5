 #include <iostream>
using namespace std;

int main() {
    char message[] = {'I', 'l', 'i', 'k', 'e', 't', 'o', 'e', 'a', 't', 'o', 'i', 'z', 'z', 'a'};
    int vowel_count = 0;

    for (int i = 0; i < sizeof(message) / sizeof(message[0]); ++i) {
        char ch = tolower(message[i]);
        if (ch == 'a'  ch == 'e'  ch == 'i'  ch == 'o'  ch == 'u') {
            vowel_count++; //
        }
    }

    cout << "Number of vowels: " << vowel_count << endl;
    return 0;
}
